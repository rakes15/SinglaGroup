<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:rsb="http://schemas.android.com/tools"
    xmlns:segmentedgroup="http://schemas.android.com/apk/res-auto"
    style="@style/CustomeLinearLayoutVerticalBothMatchParent"
    android:background="@color/row_active"
    android:gravity="center_horizontal" >
    <ScrollView
        android:layout_width="match_parent"
        android:layout_height="match_parent">
        <LinearLayout
            style="@style/CustomeLinearLayoutVerticalMatchParentWrapContent">
            <LinearLayout
            style="@style/CustomeLinearLayoutHorizontalMatchParentWrapContent"
            android:gravity="right">
            <TextView
                android:id="@+id/textView_Clear"
                style="@style/CustomSmallTextViewHeader"
                android:text="Clear"
                android:padding="@dimen/padding10dp"/>
        </LinearLayout>
            <!-- TODO: Stock -->
            <LinearLayout
                style="@style/CustomeLinearLayoutVerticalMatchParentWrapContent"
                android:visibility="visible">
                <View
                    android:layout_width="match_parent"
                    android:layout_height="1dip"
                    android:layout_marginLeft="@dimen/padding10dp"
                    android:layout_marginRight="@dimen/padding10dp"
                    android:gravity="center_horizontal|center_vertical"
                    android:background="#90a4ae"/>
                <TextView
                    style="@style/CustomMediumTextViewHeaderMatchParent"
                    android:gravity="center_horizontal"
                    android:textStyle="bold"
                    android:text="Stock" />
                <View
                    android:layout_width="match_parent"
                    android:layout_height="1dip"
                    android:layout_marginLeft="@dimen/padding10dp"
                    android:layout_marginRight="@dimen/padding10dp"
                    android:gravity="center_horizontal|center_vertical"
                    android:background="#90a4ae"/>
                <LinearLayout
                    android:id="@+id/Linear_StockType"
                    style="@style/CustomeLinearLayoutVerticalMatchParentWrapContent"
                    android:padding="@dimen/padding10dp">
                    <TextView
                        android:id="@+id/text_Stock_type"
                        style="@style/CustomSmallTextViewHeaderMatchParent"
                        android:padding="10dp"
                        android:gravity="start"
                        android:textColor="@color/colorPrimary"
                        android:text="Stock Type" />
                    <Spinner
                        android:id="@+id/spinner_StockType"
                        android:popupTheme="@android:style/ThemeOverlay.Material.Light"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:layout_marginLeft="@dimen/padding10dp"
                        android:background="@drawable/linear_layout_border"
                        android:padding="@dimen/padding10dp"
                        android:entries="@array/StockType"/>
                </LinearLayout>
                <LinearLayout
                    style="@style/CustomeLinearLayoutHorizontalMatchParentWrapContent"
                    android:padding="@dimen/padding10dp"
                    android:gravity="center_vertical">
                    <LinearLayout
                        style="@style/CustomeLinearLayoutHorizontalBothWrap">
                        <TextView
                            style="@style/CustomSmallTextViewHeader"
                            android:padding="@dimen/padding10dp"
                            android:gravity="start"
                            android:textColor="@color/colorPrimary"
                            android:text="Stock Availability" />
                    </LinearLayout>
                    <LinearLayout
                        style="@style/CustomeLinearLayoutHorizontalBothWrap">
                            <RadioGroup
                                android:id="@+id/Radio_Group_Stock_Availability"
                                android:layout_width="match_parent"
                                android:layout_height="wrap_content"
                                android:gravity="end"
                                android:orientation="horizontal">
                                <RadioButton
                                    android:id="@+id/Radio_Button_Stock_Availability_Yes"
                                    style="@style/CustomMediumTextViewHeader"
                                    android:checked="true"
                                    android:text="Yes"/>
                                <RadioButton
                                    android:id="@+id/Radio_Button_Stock_Availability_No"
                                    style="@style/CustomMediumTextViewHeader"
                                    android:text="No"/>
                                <RadioButton
                                    android:id="@+id/Radio_Button_Stock_Availability_All"
                                    style="@style/CustomMediumTextViewHeader"
                                    android:text="All"/>
                            </RadioGroup>
                    </LinearLayout>
                </LinearLayout>
                <LinearLayout
                    style="@style/CustomeLinearLayoutVerticalMatchParentWrapContent"
                    android:padding="@dimen/padding10dp"
                    android:visibility="gone"
                    android:gravity="center_vertical">
                    <TextView
                        style="@style/CustomSmallTextViewHeaderMatchParent"
                        android:text="Stock Range"
                        android:padding="@dimen/padding10dp"
                        android:textColor="@color/colorPrimary"/>
                    <LinearLayout
                        style="@style/CustomeLinearLayoutHorizontalMatchParentWrapContent"
                        android:gravity="center_vertical">
                        <LinearLayout
                            style="@style/CustomeLinearLayoutHorizontalBothWrap"
                            android:gravity="center_horizontal|center_vertical">
                            <android.support.design.widget.TextInputLayout
                                android:id="@+id/editText_Stock_From_Range"
                                android:layout_width="match_parent"
                                android:layout_height="wrap_content">
                                <EditText
                                    android:id="@+id/editText"
                                    android:layout_width="wrap_content"
                                    android:layout_height="wrap_content"
                                    android:layout_weight="1"
                                    android:hint="Min"
                                    android:ems="10"
                                    android:inputType="numberDecimal" />
                            </android.support.design.widget.TextInputLayout>
                        </LinearLayout>
                        <LinearLayout
                            style="@style/CustomeLinearLayoutHorizontalBothWrap"
                            android:gravity="center_horizontal|center_vertical">
                            <android.support.design.widget.TextInputLayout
                                android:id="@+id/editText_Stock_To_Range"
                                android:layout_width="match_parent"
                                android:layout_height="wrap_content">
                                <EditText
                                    android:id="@+id/editText_max"
                                    android:layout_width="wrap_content"
                                    android:layout_height="wrap_content"
                                    android:layout_weight="1"
                                    android:hint="Max"
                                    android:ems="10"
                                    android:inputType="numberDecimal" />
                            </android.support.design.widget.TextInputLayout>
                        </LinearLayout>
                    </LinearLayout>
                </LinearLayout>
                <RelativeLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:visibility="visible"
                    android:gravity="center_horizontal"
                    android:padding="@dimen/padding10dp">
                    <TextView
                        style="@style/CustomSmallTextViewHeaderMatchParent"
                        android:text="Stock range"
                        android:paddingLeft="@dimen/padding10dp"
                        android:paddingRight="@dimen/padding10dp"
                        android:textColor="@color/colorPrimary"/>
                    <org.florescu.android.rangeseekbar.RangeSeekBar
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:padding="@dimen/padding10dp"
                        android:id="@+id/seekbar_Stock"
                        rsb:thumbDisabled="@drawable/custom_slider_thumb"
                        rsb:thumbNormal="@drawable/custom_slider_thumb"
                        rsb:thumbPressed="@drawable/custom_slider_thumb"
                        rsb:thumbShadow="true"
                        rsb:thumbShadowBlur="3dp"
                        rsb:thumbShadowColor="@color/primary_material_light"
                        rsb:thumbShadowXOffset="1dp"
                        rsb:thumbShadowYOffset="2dp"
                        android:layout_centerVertical="true"
                        android:layout_alignParentLeft="true"
                        android:layout_alignParentStart="true"
                        android:layout_marginBottom="@dimen/padding20dp"/>
                    <TextView
                        android:id="@+id/textView_minimumRangeStock"
                        style="@style/CustomSmallTextViewContent"
                        android:layout_alignBaseline="@+id/seekbar_Stock"
                        android:paddingLeft="@dimen/padding10dp"
                        android:layout_below="@+id/seekbar_Stock"
                        android:text="0"/>
                    <TextView
                        android:id="@+id/textView_maximumRangeStock"
                        style="@style/CustomSmallTextViewContent"
                        android:paddingRight="@dimen/padding10dp"
                        android:layout_alignBaseline="@+id/seekbar_Stock"
                        android:layout_below="@+id/seekbar_Stock"
                        android:layout_alignRight="@+id/seekbar_Stock"
                        android:text="100"/>
                </RelativeLayout>
                <LinearLayout
                    style="@style/CustomeLinearLayoutHorizontalMatchParentWrapContent"
                    android:padding="@dimen/padding10dp">
                    <LinearLayout
                        style="@style/CustomeLinearLayoutHorizontalBothWrap"
                        android:gravity="center_horizontal">
                        <TextView
                            style="@style/CustomSmallTextViewHeaderMatchParent"
                            android:text="Stock Order"
                            android:padding="@dimen/padding10dp"
                            android:textColor="@color/colorPrimary"/>
                    </LinearLayout>
                    <LinearLayout
                        style="@style/CustomeLinearLayoutHorizontalBothWrap"
                        android:gravity="center_horizontal">
                        <info.hoang8f.android.segmented.SegmentedGroup
                            android:id="@+id/radioGroup_Stock_Order"
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:gravity="center_horizontal"
                            android:layout_margin="3dp"
                            android:orientation="horizontal"
                            segmentedgroup:sc_border_width="2dp"
                            segmentedgroup:sc_corner_radius="10dp">
                        <!--<TableRow android:layout_marginTop="@dimen/padding10dp">-->
                            <RadioButton
                                android:id="@+id/radioButton_Stock_LowToHigh"
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:padding="@dimen/padding5dp"
                                style="@style/RadioButton"
                                android:text="Low-high"/>
                            <RadioButton
                                android:id="@+id/radioButton_Stock_HighToLow"
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:padding="@dimen/padding5dp"
                                style="@style/RadioButton"
                                android:text="High-Low"/>
                        </info.hoang8f.android.segmented.SegmentedGroup>
                    </LinearLayout>
                </LinearLayout>
                <LinearLayout
                    style="@style/CustomeLinearLayoutHorizontalMatchParentWrapContent"
                    android:padding="@dimen/padding10dp"
                    android:gravity="center_vertical">
                    <LinearLayout
                        style="@style/CustomeLinearLayoutHorizontalBothWrap">
                        <TextView
                            style="@style/CustomSmallTextViewHeader"
                            android:padding="@dimen/padding10dp"
                            android:gravity="start"
                            android:textColor="@color/colorPrimary"
                            android:text="Image Available" />
                    </LinearLayout>
                    <LinearLayout
                        style="@style/CustomeLinearLayoutHorizontalBothWrap">
                        <RadioGroup
                            android:id="@+id/Radio_Group_Image_Available"
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:gravity="end"
                            android:orientation="horizontal">
                            <RadioButton
                                android:id="@+id/Radio_Button_ImageView_Available_Yes"
                                style="@style/CustomMediumTextViewHeader"
                                android:text="Yes"/>
                            <RadioButton
                                android:id="@+id/Radio_Button_ImageView_Available_No"
                                style="@style/CustomMediumTextViewHeader"
                                android:text="No"/>
                            <RadioButton
                                android:id="@+id/Radio_Button_ImageView_Available_All"
                                style="@style/CustomMediumTextViewHeader"
                                android:checked="true"
                                android:text="All"/>
                        </RadioGroup>
                    </LinearLayout>
                </LinearLayout>
            </LinearLayout>
            <!-- TODO: Price -->
            <LinearLayout
                style="@style/CustomeLinearLayoutVerticalMatchParentWrapContent"
                android:visibility="visible">
                <View
                    android:layout_width="match_parent"
                    android:layout_height="1dip"
                    android:layout_marginLeft="@dimen/padding10dp"
                    android:layout_marginRight="@dimen/padding10dp"
                    android:gravity="center_horizontal|center_vertical"
                    android:background="#90a4ae"/>
                <TextView
                        style="@style/CustomMediumTextViewHeaderMatchParent"
                        android:gravity="center_horizontal"
                        android:textStyle="bold"
                        android:text="Price" />
                <View
                    android:layout_width="match_parent"
                    android:layout_height="1dip"
                    android:layout_marginLeft="@dimen/padding10dp"
                    android:layout_marginRight="@dimen/padding10dp"
                    android:gravity="center_horizontal|center_vertical"
                    android:background="#90a4ae"/>
                <LinearLayout
                    style="@style/CustomeLinearLayoutVerticalMatchParentWrapContent"
                    android:padding="@dimen/padding10dp"
                    android:visibility="gone"
                    android:gravity="center_vertical">
                    <TextView
                        style="@style/CustomSmallTextViewHeaderMatchParent"
                        android:text="Price Range"
                        android:padding="@dimen/padding10dp"
                        android:textColor="@color/colorPrimary"/>
                    <LinearLayout
                        style="@style/CustomeLinearLayoutHorizontalMatchParentWrapContent"
                        android:gravity="center_vertical">
                        <LinearLayout
                            style="@style/CustomeLinearLayoutHorizontalBothWrap"
                            android:gravity="center_horizontal|center_vertical">
                            <TextView
                                android:id="@+id/textView_Price_Min"
                                style="@style/CustomSmallTextViewHeader"
                                android:padding="@dimen/padding10dp"
                                android:gravity="start"
                                android:textColor="@color/colorPrimary"
                                android:text="From :" />
                            <EditText
                                android:id="@+id/editText_Price_From_Range"
                                style="@style/CustomEditTextBorder"
                                android:padding="@dimen/padding10dp"
                                android:gravity="center_horizontal|center_vertical"
                                android:inputType="numberDecimal"
                                android:clickable="true"/>
                        </LinearLayout>
                        <LinearLayout
                            style="@style/CustomeLinearLayoutHorizontalBothWrap"
                            android:gravity="center_horizontal|center_vertical">
                            <TextView
                                android:id="@+id/textView_Price_Max"
                                style="@style/CustomSmallTextViewHeader"
                                android:padding="@dimen/padding10dp"
                                android:gravity="start"
                                android:textColor="@color/colorPrimary"
                                android:text="To :" />
                            <EditText
                                android:id="@+id/editText_Price_To_Range"
                                style="@style/CustomEditTextBorder"
                                android:padding="@dimen/padding10dp"
                                android:gravity="center_horizontal|center_vertical"
                                android:inputType="numberDecimal"
                                android:clickable="true"/>
                        </LinearLayout>
                    </LinearLayout>
                </LinearLayout>
                <RelativeLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:gravity="center_horizontal"
                    android:padding="@dimen/padding10dp">
                    <TextView
                        style="@style/CustomSmallTextViewHeaderMatchParent"
                        android:text="Price range"
                        android:paddingLeft="@dimen/padding10dp"
                        android:paddingRight="@dimen/padding10dp"
                        android:textColor="@color/colorPrimary"/>
                    <org.florescu.android.rangeseekbar.RangeSeekBar
                        android:id="@+id/seekbar_Price"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:padding="@dimen/padding10dp"
                        rsb:thumbDisabled="@drawable/custom_slider_thumb"
                        rsb:thumbNormal="@drawable/custom_slider_thumb"
                        rsb:thumbPressed="@drawable/custom_slider_thumb"
                        rsb:thumbShadow="true"
                        rsb:thumbShadowBlur="3dp"
                        rsb:thumbShadowColor="@color/primary_material_light"
                        rsb:thumbShadowXOffset="1dp"
                        rsb:thumbShadowYOffset="2dp"
                        android:layout_centerVertical="true"
                        android:layout_alignParentLeft="true"
                        android:layout_alignParentStart="true"
                        android:layout_marginBottom="@dimen/padding20dp"/>
                    <TextView
                        android:id="@+id/textView_minimumRangePrice"
                        style="@style/CustomSmallTextViewContent"
                        android:layout_alignBaseline="@+id/seekbar_Price"
                        android:paddingLeft="@dimen/padding10dp"
                        android:layout_below="@+id/seekbar_Price"
                        android:text="0"/>
                    <TextView
                        android:id="@+id/textView_maximumRangePrice"
                        style="@style/CustomSmallTextViewContent"
                        android:paddingRight="@dimen/padding10dp"
                        android:layout_alignBaseline="@+id/seekbar_Price"
                        android:layout_below="@+id/seekbar_Price"
                        android:layout_alignRight="@+id/seekbar_Price"
                        android:text="100"/>
                </RelativeLayout>
                <LinearLayout
                    style="@style/CustomeLinearLayoutHorizontalMatchParentWrapContent"
                    android:padding="@dimen/padding10dp">
                    <LinearLayout
                        style="@style/CustomeLinearLayoutHorizontalBothWrap"
                        android:gravity="center_horizontal">
                        <TextView
                            style="@style/CustomSmallTextViewHeaderMatchParent"
                            android:text="Price Order"
                            android:padding="@dimen/padding10dp"
                            android:textColor="@color/colorPrimary"/>
                    </LinearLayout>
                    <LinearLayout
                        style="@style/CustomeLinearLayoutHorizontalBothWrap"
                        android:gravity="center_horizontal">
                        <info.hoang8f.android.segmented.SegmentedGroup
                            android:id="@+id/radioGroup_Price_Order"
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:gravity="center_horizontal"
                            android:layout_margin="3dp"
                            android:orientation="horizontal"
                            segmentedgroup:sc_border_width="2dp"
                            segmentedgroup:sc_corner_radius="10dp">
                            <!--<TableRow android:layout_marginTop="@dimen/padding10dp">-->
                            <RadioButton
                                android:id="@+id/radioButton_Price_LowToHigh"
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:padding="@dimen/padding5dp"
                                style="@style/RadioButton"
                                android:text="Low-high"/>
                            <RadioButton
                                android:id="@+id/radioButton_Price_HighToLow"
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:padding="@dimen/padding5dp"
                                style="@style/RadioButton"
                                android:text="High-Low"/>
                        </info.hoang8f.android.segmented.SegmentedGroup>
                    </LinearLayout>
                </LinearLayout>
            </LinearLayout>
            <!-- TODO: Sort Days -->
            <LinearLayout
                style="@style/CustomeLinearLayoutVerticalMatchParentWrapContent"
                android:visibility="visible">
                <View
                    android:layout_width="match_parent"
                    android:layout_height="1dip"
                    android:layout_marginLeft="@dimen/padding10dp"
                    android:layout_marginRight="@dimen/padding10dp"
                    android:gravity="center_horizontal|center_vertical"
                    android:background="#90a4ae"/>
                <TextView
                    style="@style/CustomMediumTextViewHeaderMatchParent"
                    android:gravity="center_horizontal"
                    android:textStyle="bold"
                    android:text="Days" />
                <View
                    android:layout_width="match_parent"
                    android:layout_height="1dip"
                    android:layout_marginLeft="@dimen/padding10dp"
                    android:layout_marginRight="@dimen/padding10dp"
                    android:gravity="center_horizontal|center_vertical"
                    android:background="#90a4ae"/>
                <LinearLayout
                    android:id="@+id/Linear_SortDays"
                    style="@style/CustomeLinearLayoutVerticalMatchParentWrapContent"
                    android:padding="@dimen/padding10dp">
                    <TextView
                        android:id="@+id/text_SortDays"
                        style="@style/CustomSmallTextViewHeaderMatchParent"
                        android:padding="10dp"
                        android:gravity="start"
                        android:textColor="@color/colorPrimary"
                        android:text="Sort Days" />
                    <Spinner
                        android:id="@+id/spinner_SortDays"
                        android:popupTheme="@android:style/ThemeOverlay.Material.Light"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:layout_marginLeft="@dimen/padding10dp"
                        android:background="@drawable/linear_layout_border"
                        android:padding="@dimen/padding10dp"
                        android:entries="@array/SortDays"/>
                </LinearLayout>
                <LinearLayout
                    style="@style/CustomeLinearLayoutHorizontalMatchParentWrapContent"
                    android:padding="@dimen/padding10dp"
                    android:gravity="center_vertical">
                    <LinearLayout
                        style="@style/CustomeLinearLayoutHorizontalBothWrap">
                        <TextView
                            style="@style/CustomSmallTextViewHeader"
                            android:padding="@dimen/padding10dp"
                            android:gravity="start"
                            android:textColor="@color/colorPrimary"
                            android:text="Days Order" />
                    </LinearLayout>
                    <LinearLayout
                        style="@style/CustomeLinearLayoutHorizontalBothWrap"
                        android:gravity="center_horizontal">
                        <info.hoang8f.android.segmented.SegmentedGroup
                            android:id="@+id/Radio_Group_DaysType"
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:gravity="center_horizontal"
                            android:layout_margin="3dp"
                            android:orientation="horizontal"
                            segmentedgroup:sc_border_width="2dp"
                            segmentedgroup:sc_corner_radius="10dp">
                            <!--<TableRow android:layout_marginTop="@dimen/padding10dp">-->
                            <RadioButton
                                android:id="@+id/radioButton_DaysType_LowToHigh"
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:padding="@dimen/padding5dp"
                                style="@style/RadioButton"
                                android:text="Low-high"/>
                            <RadioButton
                                android:id="@+id/radioButton_DaysType_HighToLow"
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:padding="@dimen/padding5dp"
                                style="@style/RadioButton"
                                android:text="High-Low"/>
                        </info.hoang8f.android.segmented.SegmentedGroup>
                    </LinearLayout>
                </LinearLayout>
            </LinearLayout>
            <!-- TODO:Range  Days -->
            <LinearLayout
                style="@style/CustomeLinearLayoutVerticalMatchParentWrapContent"
                android:visibility="visible">
                <View
                    android:layout_width="match_parent"
                    android:layout_height="1dip"
                    android:layout_marginLeft="@dimen/padding10dp"
                    android:layout_marginRight="@dimen/padding10dp"
                    android:gravity="center_horizontal|center_vertical"
                    android:background="#90a4ae"/>
                <TextView
                    style="@style/CustomMediumTextViewHeaderMatchParent"
                    android:gravity="center_horizontal"
                    android:textStyle="bold"
                    android:text="Range Days" />
                <View
                    android:layout_width="match_parent"
                    android:layout_height="1dip"
                    android:layout_marginLeft="@dimen/padding10dp"
                    android:layout_marginRight="@dimen/padding10dp"
                    android:gravity="center_horizontal|center_vertical"
                    android:background="#90a4ae"/>
                <LinearLayout
                    android:id="@+id/Linear_RangeDays"
                    style="@style/CustomeLinearLayoutVerticalMatchParentWrapContent"
                    android:padding="@dimen/padding10dp">
                    <TextView
                        android:id="@+id/text_RangeDays"
                        style="@style/CustomSmallTextViewHeaderMatchParent"
                        android:padding="10dp"
                        android:gravity="start"
                        android:textColor="@color/colorPrimary"
                        android:text="Range Days" />
                    <Spinner
                        android:id="@+id/spinner_RangeDays"
                        android:popupTheme="@android:style/ThemeOverlay.Material.Light"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:layout_marginLeft="@dimen/padding10dp"
                        android:background="@drawable/linear_layout_border"
                        android:padding="@dimen/padding10dp"
                        android:entries="@array/SortDays"/>
                </LinearLayout>
                <LinearLayout
                    style="@style/CustomeLinearLayoutHorizontalMatchParentWrapContent"
                    android:padding="@dimen/padding10dp"
                    android:gravity="center_vertical">
                    <LinearLayout
                        style="@style/CustomeLinearLayoutHorizontalBothWrap"
                        android:gravity="center_horizontal|center_vertical">
                        <TextView
                            style="@style/CustomSmallTextViewHeader"
                            android:padding="@dimen/padding10dp"
                            android:gravity="start"
                            android:textColor="@color/colorPrimary"
                            android:text="From :" />
                        <EditText
                            android:id="@+id/editText_Range_From_Days"
                            style="@style/CustomEditTextMatchParentBorder"
                            android:padding="@dimen/padding10dp"
                            android:gravity="center_horizontal|center_vertical"
                            android:hint="Days"
                            android:visibility="gone"
                            android:ems="10"
                            android:inputType="phone">
                            <requestFocus/>
                        </EditText>
                        <LinearLayout
                            android:id="@+id/LinearLayout_Range_From_Days"
                            style="@style/CustomeLinearLayoutHorizontalBothWrap"
                            android:gravity="center_horizontal|center_vertical"/>
                    </LinearLayout>
                    <LinearLayout
                        style="@style/CustomeLinearLayoutHorizontalBothWrap"
                        android:gravity="center_horizontal|center_vertical">
                        <TextView
                            style="@style/CustomSmallTextViewHeader"
                            android:padding="@dimen/padding10dp"
                            android:gravity="start"
                            android:textColor="@color/colorPrimary"
                            android:text="To :"/>
                        <EditText
                            android:id="@+id/editText_Range_To_Days"
                            style="@style/CustomEditTextMatchParentBorder"
                            android:padding="@dimen/padding10dp"
                            android:visibility="gone"
                            android:gravity="center_horizontal|center_vertical"
                            android:hint="Days"
                            android:ems="10"
                            android:inputType="phone">
                        </EditText>
                        <LinearLayout
                            android:id="@+id/LinearLayout_Range_To_Days"
                            style="@style/CustomeLinearLayoutHorizontalBothWrap"
                            android:gravity="center_horizontal|center_vertical"/>
                    </LinearLayout>
                </LinearLayout>
            </LinearLayout>
            <!-- TODO:Button Ok -->
            <LinearLayout
                style="@style/CustomeLinearLayoutHorizontalMatchParentWrapContent"
                android:gravity="center_horizontal"
                android:padding="@dimen/padding10dp">
                <Button
                    android:id="@+id/btn_yes"
                    style="@style/CustomButtonWrap"
                    android:text="OK" />
            </LinearLayout>
        </LinearLayout>
    </ScrollView>
</LinearLayout>