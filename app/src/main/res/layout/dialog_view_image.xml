<RelativeLayout  xmlns:android="http://schemas.android.com/apk/res/android"
	style="@style/CustomeLinearLayoutVerticalBothMatchParent"
	android:background="@color/row_active"
	android:padding="@dimen/padding5dp"
	android:gravity="center_horizontal">
	<ImageView
		android:id="@+id/Image_View"
		android:layout_width="match_parent"
		android:layout_height="match_parent"
		android:layout_centerInParent="true"
		android:visibility="gone"
		android:src="@mipmap/ic_launcher"
		android:layout_above="@+id/btn_ok"
		android:layout_weight="1"
		android:layout_margin="@dimen/padding10dp"/>
	<WebView
		android:id="@+id/web_view"
		android:layout_width="match_parent"
		android:layout_height="match_parent"
		android:layout_centerInParent="true"
		android:visibility="gone"
		android:layout_above="@+id/btn_ok"/>
	<Button
		android:id="@+id/btn_ok"
		android:padding="5dp"
		android:layout_marginTop="@dimen/padding5dp"
		android:layout_alignParentBottom="true"
		android:gravity="center_horizontal|center_vertical"
		style="@style/CustomButtonMatchParent"
		android:text="OK" />
</RelativeLayout>